{
    "compilerOptions": {
        "module": "amd",
        "target": "es5",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "../../built/demo/",
        "declaration": false,
        "sourceMap": false,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noImplicitReturns": true
    }
}