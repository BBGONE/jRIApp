{
    "compilerOptions": {
        "module": "amd",
        "target": "es5",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outFile": "../../built/shared/shared.js",
        "declaration": true,
        "sourceMap": false,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noImplicitReturns": true
    }
}