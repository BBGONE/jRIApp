{
  "compilerOptions": {
    "module": "amd",
    "target": "es5",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../../built/shared/shared.js",
    "declaration": true,
    "sourceMap": false,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true
  },
  "include": [
    "*",
    "../../jriapp/jriapp_shared.d.ts",
    "../../jriapp/jriapp.d.ts",
    "../../jriapp/jriapp_ui.d.ts",
    "../../jriapp/jriapp_db.d.ts",
    "../../thirdparty/*"
  ],
  "exclude": [
    "node_modules"
  ]

}