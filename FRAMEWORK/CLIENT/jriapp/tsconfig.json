{
  "compilerOptions": {
    "module": "amd",
    "target": "es5",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../built/jriapp.js",
    "declaration": true,
    "sourceMap": false,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "lib": [ "ES2015", "ES2015.Promise", "DOM" ]
  },
  "files": [
    "jriapp.ts"
  ],
  "include": [
    "../built/jriapp_shared.d.ts",
    "global.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}