{
  "compilerOptions": {
    "module": "amd",
    "target": "es5",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../../built/jriapp_ui.js",
    "declaration": true,
    "sourceMap": false,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "lib": [ "ES2015", "ES2015.Promise", "DOM" ]
  },
  "files": [
    "jriapp_ui.ts"
  ],
  "include": [
    "../../built/jriapp_shared.d.ts",
    "../../built/jriapp.d.ts",
    "../../thirdparty/jquery.d.ts",
    "../../thirdparty/jqueryui.d.ts",
    "../../thirdparty/qtip2.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}